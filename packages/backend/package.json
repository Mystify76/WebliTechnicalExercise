  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc --build --incremental",
    "build:backend": "tsc --build --incremental",
    "build:verbose": "tsc --build --incremental --verbose",
    "start": "node -r ts-node/register ./scripts/develop.ts",
    "start:backend": "node -r ts-node/register ./scripts/develop.ts",
    "lint": "eslint '*/**/*.{ts,tsx}' --ignore-path .gitignore",
    "lint:fix": "eslint '*/**/*.{ts,tsx}' --fix --ignore-path .gitignore",
    "clean": "rm -rf dist && tsc --build --clean",
    "typeorm": "ts-node -r tsconfig-paths/register ../../node_modules/.bin/typeorm",
    "migration:create": "yarn typeorm migration:create ./src/migrations/",
    "migration:generate": "yarn typeorm migration:generate ./src/migrations/",
    "migration:revert": "yarn typeorm migration:revert -d typeOrm.config.ts "
  },
